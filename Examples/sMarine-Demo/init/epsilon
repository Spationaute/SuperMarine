FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0";
    object      epsilon;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 2 -3 0 0 0 0];

internalField   uniform 0.00754;

boundaryField
{
    impeller
    {
        type            epsilonWallFunction;
        value           uniform 0.00754;
    }

    walls
    {
        type            epsilonWallFunction;
        value           uniform 0.00754;
    }

    shaft
    {
        type            epsilonWallFunction;
        value           uniform 0.00754;
    }

    top
    {
        type            epsilonWallFunction;
        value           uniform 0.00754;
    }

    bottom
    {
        type            epsilonWallFunction;
        value           uniform 0.00754;
    }
}


// ************************************************************************* //
